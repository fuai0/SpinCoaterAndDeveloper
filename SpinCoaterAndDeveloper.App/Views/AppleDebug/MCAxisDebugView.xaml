<UserControl
    x:Class="SpinCoaterAndDeveloper.App.Views.MCAxisDebugView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:cv="clr-namespace:SpinCoaterAndDeveloper.App.Common.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:vc="clr-namespace:ValueConverters;assembly=ValueConverters"
    xmlns:viewmodels="clr-namespace:SpinCoaterAndDeveloper.App.ViewModels"
    xmlns:zc="clr-namespace:ZeroControls;assembly=ZeroControls"
    d:DataContext="{d:DesignInstance Type=viewmodels:MCAxisDebugViewModel}"
    d:DesignHeight="700"
    d:DesignWidth="1280"
    prism:ViewModelLocator.AutoWireViewModel="True"
    mc:Ignorable="d">
    <UserControl.Resources>
        <cv:Int32ToAxStsAlarmConverter x:Key="Int32ToAxStsAlarmConverter" />
        <cv:Int32ToAxStsSvonConverter x:Key="Int32ToAxStsSvonConverter" />
        <cv:Int32ToAxStsBusyConverter x:Key="Int32ToAxStsBusyConverter" />
        <cv:Int32ToAxStsArriveConverter x:Key="Int32ToAxStsArriveConverter" />
        <cv:Int32ToAxStsPosLmtConverter x:Key="Int32ToAxStsPosLmtConverter" />
        <cv:Int32ToAxStsNegLmtConverter x:Key="Int32ToAxStsNegLmtConverter" />
        <cv:Int32ToAxStsSoftPosLmtConverter x:Key="Int32ToAxStsSoftPosLmtConverter" />
        <cv:Int32ToAxStsSoftNegLmtConverter x:Key="Int32ToAxStsSoftNegLmtConverter" />
        <cv:Int32ToAxStsErrPosConverter x:Key="Int32ToAxStsErrPosConverter" />
        <cv:Int32ToAxStsEmgStopConverter x:Key="Int32ToAxStsEmgStopConverter" />
        <cv:Int32ToAxStsEcatConverter x:Key="Int32ToAxStsEcatConverter" />
        <cv:Int32ToAxStsSwAbnorConverter x:Key="Int32ToAxStsSwAbnorConverter" />
        <cv:Int32ToAxStsWarningConverter x:Key="Int32ToAxStsWarningConverter" />
        <cv:Int32ToAxStsHmConverter x:Key="Int32ToAxStsHmConverter" />
        <vc:EnumToObjectConverter x:Key="EnumToPositionConverter">
            <vc:EnumToObjectConverter.Items>
                <ResourceDictionary>
                    <sys:String x:Key="LineAxis">mm</sys:String>
                    <sys:String x:Key="RotateAxis">°(度)</sys:String>
                </ResourceDictionary>
            </vc:EnumToObjectConverter.Items>
        </vc:EnumToObjectConverter>
        <vc:EnumToObjectConverter x:Key="EnumToVelConverter">
            <vc:EnumToObjectConverter.Items>
                <ResourceDictionary>
                    <sys:String x:Key="LineAxis">mm/s</sys:String>
                    <sys:String x:Key="RotateAxis">°/s</sys:String>
                </ResourceDictionary>
            </vc:EnumToObjectConverter.Items>
        </vc:EnumToObjectConverter>
    </UserControl.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Border Margin="0,0,0,2" Background="{StaticResource AppleHeavyBackGroundBrush}">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <UniformGrid
                    Margin="0,5,5,0"
                    Columns="10"
                    FirstColumn="5">
                    <TextBlock
                        Margin="2,0,0,0"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Bottom"
                        FontSize="15"
                        Text="{DynamicResource EtherCATBusState}" />
                    <TextBlock
                        Margin="2,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Bottom"
                        FontSize="15"
                        FontWeight="Bold"
                        Text="{Binding EcatErrorCode, StringFormat=0x{0:x}}" />
                    <TextBlock
                        Margin="2,0,0,0"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Bottom"
                        FontSize="15"
                        Text="{DynamicResource FilterGroup}" />
                    <ComboBox
                        x:Name="GroupSelected"
                        Margin="2,0,0,0"
                        VerticalAlignment="Bottom"
                        HorizontalContentAlignment="Center"
                        ItemsSource="{Binding AxisGroupCollection}">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="SelectionChanged">
                                <i:InvokeCommandAction Command="{Binding AxesGroupChangedCommand}" CommandParameter="{Binding SelectedItem, ElementName=GroupSelected}" />
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </ComboBox>
                    <Button
                        Margin="2,0,0,0"
                        VerticalAlignment="Bottom"
                        Command="{Binding ShowAllAxesCommand}"
                        Content="{DynamicResource ShowAllAxes}" />
                </UniformGrid>
                <DataGrid
                    Grid.Row="1"
                    Margin="5,5,5,0"
                    materialDesign:DataGridAssist.CellPadding="10 2 10 2"
                    materialDesign:DataGridAssist.ColumnHeaderPadding="10 2 10 2"
                    materialDesign:DataGridAssist.SelectedCellBorderBrush="Transparent"
                    AutoGenerateColumns="False"
                    Background="{StaticResource AppleHeavyBackGroundBrush}"
                    CanUserAddRows="False"
                    CanUserSortColumns="False"
                    HeadersVisibility="All"
                    IsReadOnly="True"
                    ItemsSource="{Binding AxisMonitorCollection}"
                    RowHeaderWidth="1"
                    ScrollViewer.VerticalScrollBarVisibility="Visible"
                    SelectionMode="Single">
                    <DataGrid.Resources>
                        <Style
                            x:Key="DataGridColumnElementStyleDefine"
                            BasedOn="{StaticResource MaterialDesignDataGridTextColumnStyle}"
                            TargetType="{x:Type TextBlock}">
                            <Setter Property="HorizontalAlignment" Value="Center" />
                            <Setter Property="VerticalAlignment" Value="Center" />
                        </Style>
                        <Style
                            x:Key="DataGridCellOnOffStyle"
                            BasedOn="{StaticResource MaterialDesignDataGridCell}"
                            TargetType="DataGridCell">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding Content.Text, RelativeSource={RelativeSource Mode=Self}}" Value="On">
                                    <Setter Property="Foreground" Value="{StaticResource SecondaryAccentBrush}" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding Content.Text, RelativeSource={RelativeSource Mode=Self}}" Value="Off">
                                    <Setter Property="Foreground" Value="{StaticResource SecondaryAccentForegroundBrush}" />
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </DataGrid.Resources>
                    <DataGrid.Columns>
                        <DataGridTextColumn
                            Width="*"
                            Binding="{Binding Number, Mode=OneWay}"
                            EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnEditingStyle}"
                            ElementStyle="{StaticResource DataGridColumnElementStyleDefine}">
                            <DataGridTextColumn.HeaderStyle>
                                <Style BasedOn="{StaticResource MaterialDesignDataGridColumnHeader}" TargetType="{x:Type DataGridColumnHeader}">
                                    <Setter Property="Content" Value="{DynamicResource Number}" />
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                </Style>
                            </DataGridTextColumn.HeaderStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn
                            Width="*"
                            Binding="{Binding ShowOnUIName, Mode=OneWay}"
                            EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnEditingStyle}"
                            ElementStyle="{StaticResource DataGridColumnElementStyleDefine}">
                            <DataGridTextColumn.HeaderStyle>
                                <Style BasedOn="{StaticResource MaterialDesignDataGridColumnHeader}" TargetType="{x:Type DataGridColumnHeader}">
                                    <Setter Property="Content" Value="{DynamicResource AxisName}" />
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                </Style>
                            </DataGridTextColumn.HeaderStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn
                            Width="*"
                            Binding="{Binding EncPos, StringFormat={}{0:f4}, Mode=OneWay}"
                            EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnEditingStyle}"
                            ElementStyle="{StaticResource DataGridColumnElementStyleDefine}">
                            <DataGridTextColumn.HeaderStyle>
                                <Style BasedOn="{StaticResource MaterialDesignDataGridColumnHeader}" TargetType="{x:Type DataGridColumnHeader}">
                                    <Setter Property="Content" Value="{DynamicResource AxisPosition}" />
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                </Style>
                            </DataGridTextColumn.HeaderStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn
                            Width="*"
                            Binding="{Binding Status, Converter={StaticResource Int32ToAxStsNegLmtConverter}, Mode=OneWay}"
                            CellStyle="{StaticResource DataGridCellOnOffStyle}"
                            EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnEditingStyle}"
                            ElementStyle="{StaticResource DataGridColumnElementStyleDefine}">
                            <DataGridTextColumn.HeaderStyle>
                                <Style BasedOn="{StaticResource MaterialDesignDataGridColumnHeader}" TargetType="{x:Type DataGridColumnHeader}">
                                    <Setter Property="Content" Value="{DynamicResource NegativeLimit}" />
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                </Style>
                            </DataGridTextColumn.HeaderStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn
                            Width="*"
                            Binding="{Binding Status, Converter={StaticResource Int32ToAxStsHmConverter}, Mode=OneWay}"
                            CellStyle="{StaticResource DataGridCellOnOffStyle}"
                            EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnEditingStyle}"
                            ElementStyle="{StaticResource DataGridColumnElementStyleDefine}">
                            <DataGridTextColumn.HeaderStyle>
                                <Style BasedOn="{StaticResource MaterialDesignDataGridColumnHeader}" TargetType="{x:Type DataGridColumnHeader}">
                                    <Setter Property="Content" Value="{DynamicResource Origin}" />
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                </Style>
                            </DataGridTextColumn.HeaderStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn
                            Width="*"
                            Binding="{Binding Status, Converter={StaticResource Int32ToAxStsPosLmtConverter}, Mode=OneWay}"
                            CellStyle="{StaticResource DataGridCellOnOffStyle}"
                            EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnEditingStyle}"
                            ElementStyle="{StaticResource DataGridColumnElementStyleDefine}">
                            <DataGridTextColumn.HeaderStyle>
                                <Style BasedOn="{StaticResource MaterialDesignDataGridColumnHeader}" TargetType="{x:Type DataGridColumnHeader}">
                                    <Setter Property="Content" Value="{DynamicResource PositiveLimit}" />
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                </Style>
                            </DataGridTextColumn.HeaderStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn
                            Width="*"
                            Binding="{Binding PrfPos, StringFormat={}{0:f4}, Mode=OneWay}"
                            EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnEditingStyle}"
                            ElementStyle="{StaticResource DataGridColumnElementStyleDefine}">
                            <DataGridTextColumn.HeaderStyle>
                                <Style BasedOn="{StaticResource MaterialDesignDataGridColumnHeader}" TargetType="{x:Type DataGridColumnHeader}">
                                    <Setter Property="Content" Value="{DynamicResource TargetLocation}" />
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                </Style>
                            </DataGridTextColumn.HeaderStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn
                            Width="*"
                            Binding="{Binding PrfVel, StringFormat={}{0:f4}, Mode=OneWay}"
                            EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnEditingStyle}"
                            ElementStyle="{StaticResource DataGridColumnElementStyleDefine}">
                            <DataGridTextColumn.HeaderStyle>
                                <Style BasedOn="{StaticResource MaterialDesignDataGridColumnHeader}" TargetType="{x:Type DataGridColumnHeader}">
                                    <Setter Property="Content" Value="{DynamicResource TargetVel}" />
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                </Style>
                            </DataGridTextColumn.HeaderStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn
                            Width="*"
                            Binding="{Binding Status, Converter={StaticResource Int32ToAxStsSvonConverter}, Mode=OneWay}"
                            CellStyle="{StaticResource DataGridCellOnOffStyle}"
                            EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnEditingStyle}"
                            ElementStyle="{StaticResource DataGridColumnElementStyleDefine}">
                            <DataGridTextColumn.HeaderStyle>
                                <Style BasedOn="{StaticResource MaterialDesignDataGridColumnHeader}" TargetType="{x:Type DataGridColumnHeader}">
                                    <Setter Property="Content" Value="{DynamicResource ServoOn}" />
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                </Style>
                            </DataGridTextColumn.HeaderStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn
                            Width="*"
                            Binding="{Binding Status, Converter={StaticResource Int32ToAxStsAlarmConverter}, Mode=OneWay}"
                            CellStyle="{StaticResource DataGridCellOnOffStyle}"
                            EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnEditingStyle}"
                            ElementStyle="{StaticResource DataGridColumnElementStyleDefine}">
                            <DataGridTextColumn.HeaderStyle>
                                <Style BasedOn="{StaticResource MaterialDesignDataGridColumnHeader}" TargetType="{x:Type DataGridColumnHeader}">
                                    <Setter Property="Content" Value="{DynamicResource AxisAlarm}" />
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                </Style>
                            </DataGridTextColumn.HeaderStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn
                            Width="*"
                            Binding="{Binding Status, Converter={StaticResource Int32ToAxStsBusyConverter}, Mode=OneWay}"
                            CellStyle="{StaticResource DataGridCellOnOffStyle}"
                            EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnEditingStyle}"
                            ElementStyle="{StaticResource DataGridColumnElementStyleDefine}">
                            <DataGridTextColumn.HeaderStyle>
                                <Style BasedOn="{StaticResource MaterialDesignDataGridColumnHeader}" TargetType="{x:Type DataGridColumnHeader}">
                                    <Setter Property="Content" Value="{DynamicResource AxisBusySts}" />
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                </Style>
                            </DataGridTextColumn.HeaderStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn
                            Width="*"
                            Binding="{Binding ErrorCode, StringFormat=0x{0:x}, Mode=OneWay}"
                            CellStyle="{StaticResource DataGridCellOnOffStyle}"
                            EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnEditingStyle}"
                            ElementStyle="{StaticResource DataGridColumnElementStyleDefine}">
                            <DataGridTextColumn.HeaderStyle>
                                <Style BasedOn="{StaticResource MaterialDesignDataGridColumnHeader}" TargetType="{x:Type DataGridColumnHeader}">
                                    <Setter Property="Content" Value="{DynamicResource ErrorCode}" />
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                </Style>
                            </DataGridTextColumn.HeaderStyle>
                        </DataGridTextColumn>
                    </DataGrid.Columns>
                </DataGrid>
            </Grid>
        </Border>
        <Border
            Grid.Row="1"
            Margin="0,2,0,0"
            Background="{StaticResource AppleHeavyBackGroundBrush}">
            <ItemsControl ItemsSource="{Binding AxisMonitorCollection}">
                <ItemsControl.ItemsPanel>
                    <ItemsPanelTemplate>
                        <WrapPanel IsItemsHost="True" />
                    </ItemsPanelTemplate>
                </ItemsControl.ItemsPanel>
                <ItemsControl.Template>
                    <ControlTemplate>
                        <ScrollViewer CanContentScroll="False" VerticalScrollBarVisibility="Auto">
                            <ItemsPresenter />
                        </ScrollViewer>
                    </ControlTemplate>
                </ItemsControl.Template>
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <Grid Width="402" Margin="5,5,5,20">
                            <Grid.Resources>
                                <Style BasedOn="{StaticResource MaterialDesignTextBlock}" TargetType="TextBlock">
                                    <Setter Property="HorizontalAlignment" Value="Center" />
                                    <Setter Property="VerticalAlignment" Value="Center" />
                                </Style>
                                <Style BasedOn="{StaticResource MaterialDesignTextBox}" TargetType="TextBox">
                                    <Setter Property="VerticalAlignment" Value="Center" />
                                    <Setter Property="Block.TextAlignment" Value="Center" />
                                    <Setter Property="Padding" Value="0" />
                                </Style>
                            </Grid.Resources>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Border
                                Grid.Row="0"
                                Grid.RowSpan="11"
                                Grid.Column="4"
                                Margin="5,10,0,3"
                                BorderBrush="#939393"
                                BorderThickness="0,0,1,0" />
                            <TextBlock
                                Grid.Row="0"
                                Grid.Column="0"
                                Grid.ColumnSpan="3"
                                Margin="0"
                                Padding="0"
                                HorizontalAlignment="Left"
                                FontSize="17"
                                FontWeight="Bold"
                                Text="{Binding ShowOnUIName}" />
                            <Button
                                Grid.Row="0"
                                Grid.Column="3"
                                Margin="2,0,0,2"
                                Padding="0"
                                Command="{Binding DataContext.AxisClearErrorCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ItemsControl}}"
                                CommandParameter="{Binding}"
                                Content="{DynamicResource ClearError}" />
                            <TextBlock
                                Grid.Row="1"
                                Grid.Column="0"
                                Padding="0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Bottom"
                                Text="{DynamicResource JogVelInfo}" />
                            <zc:NumericUpDown
                                Grid.Row="2"
                                Grid.Column="0"
                                VerticalAlignment="Center"
                                HorizontalContentAlignment="Center"
                                BorderThickness="0,0,0,1"
                                CornerRadius="0"
                                DecimalPlaces="2"
                                Increment="1"
                                Minimum="1"
                                Style="{StaticResource ZeroNumericUpDown}"
                                Value="{Binding JogVel}">
                                <i:Interaction.Triggers>
                                    <i:EventTrigger EventName="LostFocus">
                                        <i:InvokeCommandAction Command="{Binding DataContext.UpdateJogVelCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ItemsControl}}" CommandParameter="{Binding}" />
                                    </i:EventTrigger>
                                </i:Interaction.Triggers>
                            </zc:NumericUpDown>
                            <TextBlock
                                Grid.Row="2"
                                Grid.Column="1"
                                Margin="2,0,2,0"
                                HorizontalAlignment="Left"
                                Text="{Binding Type, Converter={StaticResource EnumToVelConverter}}" />
                            <Button
                                Grid.Row="1"
                                Grid.Column="2"
                                Margin="0,0,2,2"
                                Content="{materialDesign:PackIcon Kind=ChevronTripleLeft,
                                                                  Size=26}">
                                <i:Interaction.Triggers>
                                    <i:EventTrigger EventName="PreviewMouseLeftButtonDown">
                                        <i:InvokeCommandAction Command="{Binding DataContext.JogNegativeCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ItemsControl}}" CommandParameter="{Binding}" />
                                    </i:EventTrigger>
                                    <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                                        <i:InvokeCommandAction Command="{Binding DataContext.JogMoveStopCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ItemsControl}}" CommandParameter="{Binding}" />
                                    </i:EventTrigger>
                                </i:Interaction.Triggers>
                            </Button>
                            <Button
                                Grid.Row="1"
                                Grid.Column="3"
                                Margin="2,0,0,2"
                                Content="{materialDesign:PackIcon Kind=ChevronTripleRight,
                                                                  Size=26}">
                                <i:Interaction.Triggers>
                                    <i:EventTrigger EventName="PreviewMouseLeftButtonDown">
                                        <i:InvokeCommandAction Command="{Binding DataContext.JogPositiveCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ItemsControl}}" CommandParameter="{Binding}" />
                                    </i:EventTrigger>
                                    <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                                        <i:InvokeCommandAction Command="{Binding DataContext.JogMoveStopCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ItemsControl}}" CommandParameter="{Binding}" />
                                    </i:EventTrigger>
                                </i:Interaction.Triggers>
                            </Button>
                            <Button
                                Grid.Row="2"
                                Grid.Column="2"
                                Margin="0,2,2,0"
                                Padding="0"
                                Command="{Binding DataContext.AxisServoOnCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ItemsControl}}"
                                CommandParameter="{Binding}"
                                Content="{DynamicResource AxisEnable}" />
                            <Button
                                Grid.Row="2"
                                Grid.Column="3"
                                Margin="2,2,0,0"
                                Padding="0"
                                Command="{Binding DataContext.AxisServoOffCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ItemsControl}}"
                                CommandParameter="{Binding}"
                                Content="{DynamicResource AxisDisable}" />
                            <TextBlock
                                Grid.Row="3"
                                Grid.Column="0"
                                Margin="0"
                                Padding="0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Bottom"
                                Text="{DynamicResource StepMoveInfo}" />
                            <zc:NumericUpDown
                                Grid.Row="4"
                                Grid.Column="0"
                                VerticalAlignment="Center"
                                HorizontalContentAlignment="Center"
                                BorderThickness="0,0,0,1"
                                CornerRadius="0"
                                DecimalPlaces="3"
                                Increment="0.01"
                                Style="{StaticResource ZeroNumericUpDown}"
                                Value="{Binding RelMoveDistance}" />
                            <TextBlock
                                Grid.Row="4"
                                Grid.Column="1"
                                Margin="2,0,2,0"
                                HorizontalAlignment="Left"
                                Text="{Binding Type, Converter={StaticResource EnumToPositionConverter}}" />
                            <Button
                                Grid.Row="3"
                                Grid.RowSpan="2"
                                Grid.Column="2"
                                Margin="0,0,2,2"
                                Padding="0"
                                VerticalAlignment="Bottom"
                                Command="{Binding DataContext.RelMoveCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ItemsControl}}"
                                CommandParameter="{Binding}"
                                Content="{DynamicResource StepMoveStartButton}" />
                            <Button
                                Grid.Row="3"
                                Grid.RowSpan="2"
                                Grid.Column="3"
                                Margin="2,0,0,2"
                                Padding="0"
                                VerticalAlignment="Bottom"
                                Command="{Binding DataContext.RelMoveStopCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ItemsControl}}"
                                CommandParameter="{Binding}"
                                Content="{DynamicResource StepMoveStopButton}" />
                            <Button
                                Grid.Row="5"
                                Grid.RowSpan="2"
                                Grid.Column="2"
                                Margin="0,2,2,0"
                                Padding="0"
                                VerticalAlignment="Bottom"
                                Command="{Binding DataContext.HomingStartCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ItemsControl}}"
                                CommandParameter="{Binding}"
                                Content="{DynamicResource HomingInfo}" />
                            <Button
                                Grid.Row="5"
                                Grid.RowSpan="2"
                                Grid.Column="3"
                                Margin="2,2,0,0"
                                Padding="0"
                                VerticalAlignment="Bottom"
                                Command="{Binding DataContext.HomingStopCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ItemsControl}}"
                                CommandParameter="{Binding}"
                                Content="{DynamicResource StopHomingInfo}" />
                            <TextBlock
                                Grid.Row="5"
                                Grid.Column="0"
                                Margin="0"
                                Padding="0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Bottom"
                                Text="{DynamicResource ReciprocateStartPoint}" />
                            <TextBox
                                Grid.Row="6"
                                Grid.Column="0"
                                Padding="0"
                                Text="{Binding ReciprocateStartPoint}" />
                            <TextBlock
                                Grid.Row="6"
                                Grid.Column="1"
                                Margin="2,0,2,0"
                                Padding="0"
                                HorizontalAlignment="Left"
                                Text="{Binding Type, Converter={StaticResource EnumToPositionConverter}}" />
                            <TextBlock
                                Grid.Row="7"
                                Grid.Column="0"
                                Margin="0"
                                Padding="0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Bottom"
                                Text="{DynamicResource ReciprocateEndPoint}" />
                            <TextBox
                                Grid.Row="8"
                                Grid.Column="0"
                                Text="{Binding ReciprocateEndPoint}" />
                            <TextBlock
                                Grid.Row="8"
                                Grid.Column="1"
                                Margin="2,0,2,0"
                                Padding="0"
                                HorizontalAlignment="Left"
                                Text="{Binding Type, Converter={StaticResource EnumToPositionConverter}}" />
                            <TextBlock
                                Grid.Row="9"
                                Grid.Column="0"
                                Margin="0"
                                Padding="0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Bottom"
                                Text="{DynamicResource ReciprocatePauseTime}" />
                            <TextBox
                                Grid.Row="10"
                                Grid.Column="0"
                                Text="{Binding ReciprocatePauseTime}" />
                            <TextBlock
                                Grid.Row="10"
                                Grid.Column="1"
                                Margin="2,0,2,0"
                                Padding="0"
                                HorizontalAlignment="Left"
                                Text="ms" />
                            <Button
                                Grid.Row="9"
                                Grid.RowSpan="2"
                                Grid.Column="2"
                                Margin="0,2,2,0"
                                Padding="0"
                                VerticalAlignment="Bottom"
                                Command="{Binding DataContext.ReciprocateStartCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ItemsControl}}"
                                CommandParameter="{Binding}"
                                Content="{DynamicResource ReciprocateStart}" />
                            <Button
                                Grid.Row="9"
                                Grid.RowSpan="2"
                                Grid.Column="3"
                                Margin="2,0,0,0"
                                Padding="0"
                                VerticalAlignment="Bottom"
                                Command="{Binding DataContext.ReciprocateStopCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ItemsControl}}"
                                CommandParameter="{Binding}"
                                Content="{DynamicResource ReciprocateStop}" />
                        </Grid>
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>
        </Border>

    </Grid>
</UserControl>
