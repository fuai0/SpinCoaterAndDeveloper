<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="DataBaseServiceConfigGroup" type="DataBaseService.Service.DataBaseServiceConfigurationSection,DataBaseService"/>
	</configSections>
	<DataBaseServiceConfigGroup>
		<!-- DbType: MySql = 0,SqlServer = 1,Sqlite = 2,Oracle = 3,PostgreSQL = 4,Dm = 5,Kdbndp = 6,Oscar = 7,MySqlConnector = 8,Access = 9,OpenGauss = 10,QuestDB = 11,HG = 12,ClickHouse = 13,GBase = 14,Odbc = 0xF,Custom = 900 -->
		<!-- controlFileQuantity:保留备份表数量 -->
		<!-- MainDb为框架内置数据库,不能修改名称,debugMode:是否输出Sql语句-->
		<DataBaseServiceConfig iocName="MainDb" serviceName="MySql" dbType="MySql" connectionString="server=127.0.0.1;port=3306;user id=root;password=123456;persistsecurityinfo=True;database=SpinCoaterAndDeveloper;SslMode=None;allowuservariables=True;" debugMode="False" controlFileQuantity="2" backupDBPath=".\DbBackup; " dbBackupControlFileQuantity="10"/>
	</DataBaseServiceConfigGroup>
</configuration>

<!--Init方法的参数顺序-->
<!--void Init(DbType dbType,string connectionString,int controlFileQuantity,int dbBackupControlFileQuantity,string backupDBPath,bool debugMode,string serviceName);-->


<!--关于配置参数的说明-->

<!--1.iocName="MainDb"  MainDb为框架内置数据库,不能修改名称
2.DbType: MySql = 0,SqlServer = 1,Sqlite = 2,Oracle = 3,PostgreSQL = 4,Dm = 5,Kdbndp = 6,Oscar = 7,MySqlConnector = 8,Access = 9,OpenGauss = 10,QuestDB = 11,HG = 12,ClickHouse = 13,GBase = 14,Odbc = 0xF,Custom = 900
3.serviceName:数据库服务名,不区分大小写
4.controlFileQuantity:保留备份表数量
5.dbBackupControlFileQuantity:保留备份数据库文件数量
6.backupDBPath:备份数据库路径,支持多个路径,以;或；隔开)
例如:  backupDBPath="D:\DbBackup\;  .\DbBackup；E:\DbBackup  ; "  同时在D:\DbBackup与.\DbBackup与E:\DbBackup三个路径下保存
7.debugMode:是否输出Sql语句-->